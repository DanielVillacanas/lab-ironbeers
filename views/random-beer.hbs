<div class="randomBeer">
{{#each responseFromAPI}}
  
    {{> beerpartial this }}
    <div style="display: flex flex-direction:colum;">

      <h4>Food Pairing</h4>
      <ul>
        {{#each food_pairing}}
          <li>{{this}}</li>
        {{/each}}
      </ul>
      <p>
        <h4>Tips</h4>
        {{brewers_tips}}
      </p>

    </div>
  
{{/each}}
</div>